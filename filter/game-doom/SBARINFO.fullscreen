                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_base.png", 48, -41; }

                                    IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/timers/goodolhud_timer_gray.png", 50, -39; }
                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_gray.png", 50, -39; }

                                    DrawNumber 4, "fonts/goodolhud_font.bmf", Gray, PowerupTime BlurSphere, Alignment(Center), DrawShadow, 53, -57, 2;
                                }
                            } else {
                                InInventory PowerInvulnerable
                                {
                                    IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/timers/goodolhud_timer_base.png", 48, -41; }
                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_base.png", 48, -41; }

                                    IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/timers/goodolhud_timer_gray.png", 50, -39; }
                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_gray.png", 50, -39; }

                                    DrawNumber 4, "fonts/goodolhud_font.bmf", Gray, PowerupTime BlurSphere, Alignment(Center), DrawShadow, 53, -57, 2;
                                } else {
                                    IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/timers/goodolhud_timer_base.png", 26, -41; }
                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_basesideleft.png", 26, -41; }

                                    IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/timers/goodolhud_timer_gray.png", 28, -39; }
                                    else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/timers/goodolhud_timer_gray.png", 28, -39; }

                                    DrawNumber 4, "fonts/goodolhud_font.bmf", Gray, PowerupTime BlurSphere, Alignment(Center), DrawShadow, 31, -57, 2;
                                }
                            }
                        }
                    }
                }
            }
        }
    }

    // Top right.

    // Timer
    IfCVarInt goh_showtimer, 1, equal { DrawString "fonts/goodolhud_font.bmf", Untranslated, Time, -70, 0, 2, Alignment(Right), DrawShadow; }

    GameMode SinglePlayer, Cooperative
    {
        // Monsters
        IfCVarInt goh_showmonstercounter, 1, equal
        {
            IfCVarInt goh_showtimer, 1, equal
            {
                DrawString "fonts/goodolhud_font.bmf", Brick, "$GOODOLHUD_MONSTERS", -102, 16, 2, Alignment(Right), DrawShadow;
                DrawString "fonts/goodolhud_font.bmf", Brick, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                DrawNumber 5, "fonts/goodolhud_font.bmf", Brick, Kills, Alignment(Right), DrawShadow, -54, 16, 2;
                DrawNumber 5, "fonts/goodolhud_font.bmf", Brick, Monsters, Alignment(Left), DrawShadow, -46, 16, 2;
            } else {
                DrawString "fonts/goodolhud_font.bmf", Brick, "$GOODOLHUD_MONSTERS", -102, 0, 2, Alignment(Right), DrawShadow;
                DrawString "fonts/goodolhud_font.bmf", Brick, "$GOODOLHUD_SEPARATOR", -46, 0, 2, Alignment(Right), DrawShadow;

                DrawNumber 5, "fonts/goodolhud_font.bmf", Brick, Kills, Alignment(Right), DrawShadow, -54, 0, 2;
                DrawNumber 5, "fonts/goodolhud_font.bmf", Brick, Monsters, Alignment(Left), DrawShadow, -46, 0, 2;
            }
        }

        // Secrets
        IfCVarInt goh_showsecretcounter, 1, equal
        {
            IfCVarInt goh_showmonstercounter, 1, equal
            {
                IfCVarInt goh_showtimer, 1, equal
                {
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SECRETS", -102, 32, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SEPARATOR", -46, 32, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, Secrets, Alignment(Right), DrawShadow, -54, 32, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, TotalSecrets, Alignment(Left), DrawShadow, -46, 32, 2;
                } else {
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SECRETS", -102, 16, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, Secrets, Alignment(Right), DrawShadow, -54, 16, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, TotalSecrets, Alignment(Left), DrawShadow, -46, 16, 2;
                }
            } else {
                IfCVarInt goh_showtimer, 1, equal
                {
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SECRETS", -102, 16, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, Secrets, Alignment(Right), DrawShadow, -54, 16, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, TotalSecrets, Alignment(Left), DrawShadow, -46, 16, 2;
                } else {
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SECRETS", -102, 0, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", Yellow, "$GOODOLHUD_SEPARATOR", -46, 0, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, Secrets, Alignment(Right), DrawShadow, -54, 0, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", Yellow, TotalSecrets, Alignment(Left), DrawShadow, -46, 0, 2;
                }
            }
        }

        // Items
        IfCVarInt goh_showitemcounter, 1, equal
        {
            IfCVarInt goh_showsecretcounter, 1, equal
            {
                IfCVarInt goh_showmonstercounter, 1, equal
                {
                    IfCVarInt goh_showtimer, 1, equal
                    {
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 48, 2, Alignment(Right), DrawShadow;
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 48, 2, Alignment(Right), DrawShadow;

                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 48, 2;
                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 48, 2;
                    } else {
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 32, 2, Alignment(Right), DrawShadow;
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 32, 2, Alignment(Right), DrawShadow;

                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 32, 2;
                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 32, 2;
                    }
                } else {
                    IfCVarInt goh_showtimer, 1, equal
                    {
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 32, 2, Alignment(Right), DrawShadow;
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 32, 2, Alignment(Right), DrawShadow;

                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 32, 2;
                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 32, 2;
                    } else {
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 16, 2, Alignment(Right), DrawShadow;
                        DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 16, 2;
                        DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 16, 2;
                    }
                }
            } else IfCVarInt goh_showmonstercounter, 1, equal {
                IfCVarInt goh_showtimer, 1, equal
                {
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 32, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 32, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 32, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 32, 2;
                } else {
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 16, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 16, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 16, 2;
                }
            } else {
                IfCVarInt goh_showtimer, 1, equal
                {
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 16, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 16, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 16, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 16, 2;
                } else {
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_ITEMS", -102, 0, 2, Alignment(Right), DrawShadow;
                    DrawString "fonts/goodolhud_font.bmf", LightBlue, "$GOODOLHUD_SEPARATOR", -46, 0, 2, Alignment(Right), DrawShadow;

                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, Items, Alignment(Right), DrawShadow, -54, 0, 2;
                    DrawNumber 5, "fonts/goodolhud_font.bmf", LightBlue, TotalItems, Alignment(Left), DrawShadow, -46, 0, 2;
                }
            }
        }

        // Keys
        IfCVarInt goh_showtimer, 1, equal
        {
            IfCVarInt goh_showmonstercounter, 1, equal
            {
                IfCVarInt goh_showsecretcounter, 1, equal
                {
                    IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 68; }
                    else { DrawKeyBar 100, Vertical, Auto, -26, 52; }
                }
                else IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 52; }
                else { DrawKeyBar 100, Vertical, Auto, -26, 36; }
            } else IfCVarInt goh_showsecretcounter, 1, equal {
                IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 52; }
                else { DrawKeyBar 100, Vertical, Auto, -26, 36; }
            }
            else IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 36; }
            else { DrawKeyBar 100, Vertical, Auto, -26, 20; }
        } else {
            IfCVarInt goh_showmonstercounter, 1, equal
            {
                IfCVarInt goh_showsecretcounter, 1, equal
                {
                    IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 52; }
                    else { DrawKeyBar 100, Vertical, Auto, -26, 36; }
                }
                else IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 36; }
                else { DrawKeyBar 100, Vertical, Auto, -26, 20; }
            } else IfCVarInt goh_showsecretcounter, 1, equal {
                IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 36; }
                else { DrawKeyBar 100, Vertical, Auto, -26, 20; }
            }
            else IfCVarInt goh_showitemcounter, 1, equal { DrawKeyBar 100, Vertical, Auto, -26, 20; }
            else { DrawKeyBar 100, Vertical, Auto, -26, 4; }
        }
    }

    GameMode Deathmatch, TeamGame
    {
        // Frag count
        IfCVarInt goh_showtimer, 1, equal { DrawNumber 10, "fonts/goodolhud_font.bmf", Untranslated, Frags, DrawShadow, -6, 16, 2; }
        else { DrawNumber 10, "fonts/goodolhud_font.bmf", Untranslated, Frags, DrawShadow, -6, 0, 2; }
    }

    // Top center.

    // Oxygen
    IfCVarInt goh_showairtimer, 1, equal
    {
        IfWaterLevel 3
        {
            IfCVarInt not goh_theme, 1 { DrawImage "graphics/hud/theme0/bars/goodolhud_bar_base.png", -73 + Center, 60; }
            else IfCVarInt goh_theme, 1 { DrawImage "graphics/hud/theme1/bars/goodolhud_bar_base.png", -73 + Center, 60; }

            IfCVarInt not goh_theme, 1 { DrawBar "graphics/hud/theme0/bars/goodolhud_bar_blue.png", "graphics/hud/theme0/bars/goodolhud_bar_blank.png", AirTime, Horizontal, -72 + Center, 61; }
            else IfCVarInt goh_theme, 1 { DrawBar "graphics/hud/theme1/bars/goodolhud_bar_blue.png", "graphics/hud/theme1/bars/goodolhud_bar_blank.png", AirTime, Horizontal, -72 + Center, 61; }

            DrawString "fonts/goodolhud_font.bmf", Blue, "$GOODOLHUD_OXYGEN", -79 + Center, 63, 2, Alignment(Right), DrawShadow;
            DrawNumber 4, "fonts/goodolhud_font.bmf", Blue, AirTime, Alignment(Left), DrawShadow, 82 + Center, 63, 2;
        }
    }
}
